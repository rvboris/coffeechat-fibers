@css {
  .clearfix:after {
    content: ".";
    display: block;
    clear: both;
    visibility: hidden;
    line-height: 0;
    height: 0;
  }

  .clearfix { display: inline-block }
  html[xmlns] .clearfix { display: block }
  * html .clearfix { height: 1% }

  @font-face {
    font-family: 'UniversCyrLight';
    src: url('/fonts/uvyl_-webfont.eot');
    src: url('/fonts/uvyl_-webfont.eot?#iefix') format('embedded-opentype'),
       url('/fonts/uvyl_-webfont.woff') format('woff'),
       url('/fonts/uvyl_-webfont.ttf') format('truetype'),
       url('/fonts/uvyl_-webfont.svg#UniversCyrLight') format('svg');
    font-weight: normal;
    font-style: normal;
  }

  pre, p {
    white-space: pre;
    white-space: pre-wrap;
    white-space: pre-line;
    white-space: -pre-wrap;
    white-space: -o-pre-wrap;
    white-space: -moz-pre-wrap;
    white-space: -hp-pre-wrap;
    word-wrap: break-word;
  }

  input::-webkit-outer-spin-button,
  input::-webkit-inner-spin-button {
    -webkit-appearance: none;
    margin: 0;
  }

  .ui-resizable { position: relative;}
  .ui-resizable-handle { position: absolute;font-size: 0.1px;z-index: 99999; display: block; }
  .ui-resizable-disabled .ui-resizable-handle, .ui-resizable-autohide .ui-resizable-handle { display: none; }
  .ui-resizable-n { cursor: n-resize; height: 7px; width: 100%; top: -5px; left: 0; }
  .ui-resizable-s { cursor: s-resize; height: 7px; width: 100%; bottom: -5px; left: 0; }
  .ui-resizable-e { cursor: e-resize; width: 7px; right: -5px; top: 0; height: 100%; }
  .ui-resizable-w { cursor: w-resize; width: 7px; left: -5px; top: 0; height: 100%; }
  .ui-resizable-se { cursor: se-resize; width: 12px; height: 12px; right: 1px; bottom: 1px; }
  .ui-resizable-sw { cursor: sw-resize; width: 9px; height: 9px; left: -5px; bottom: -5px; }
  .ui-resizable-nw { cursor: nw-resize; width: 9px; height: 9px; left: -5px; top: -5px; }
  .ui-resizable-ne { cursor: ne-resize; width: 9px; height: 9px; right: -5px; top: -5px;}

  #bottom input::-webkit-input-placeholder { color: #89C071; }
  #bottom input:-moz-placeholder { color: #89C071; }

  #bottom, #bottom input[type=text] {
    background-color: #3D6C1A;
    background-image: -moz-linear-gradient(top, #3D6C1A, #315715);
    background-image: -o-linear-gradient(top, #3D6C1A, #315715);
    background-image: -webkit-gradient(linear,left top,left bottom,color-stop(0, #3D6C1A),color-stop(1, #315715));
    background-image: -webkit-linear-gradient(#3D6C1A, #315715);
    background-image: linear-gradient(top, #3D6C1A, #315715);
  }

  #bottom input[type=text]:focus {
    background-color: #254210;
    background-image: -moz-linear-gradient(top, #315715, #254210);
    background-image: -o-linear-gradient(top, #315715, #254210);
    background-image: -webkit-gradient(linear,left top,left bottom,color-stop(0, #315715),color-stop(1, #254210));
    background-image: -webkit-linear-gradient(#315715, #254210);
    background-image: linear-gradient(top, #315715, #254210);
  }

  #bottom button[type=submit], #bottom button#smile, #bottom button#help {
    background-color: #035C56;
    background-image: -moz-linear-gradient(top, #035C56, #02433F);
    background-image: -o-linear-gradient(top, #035C56, #02433F);
    background-image: -webkit-gradient(linear,left top,left bottom,color-stop(0, #035C56),color-stop(1, #02433F));
    background-image: -webkit-linear-gradient(#035C56, #02433F);
    background-image: linear-gradient(top, #035C56, #02433F);
  }

  #bottom button[type=submit]:hover, #bottom button#smile:hover, #bottom button#smile.pressed, #bottom button#help:hover, #bottom button#help.pressed {
    background-color: #02433F;
    background-image: -moz-linear-gradient(top, #02433F, #012A28);
    background-image: -o-linear-gradient(top, #02433F, #012A28);
    background-image: -webkit-gradient(linear,left top,left bottom,color-stop(0, #02433F),color-stop(1, #012A28));
    background-image: -webkit-linear-gradient(#02433F, #012A28);
    background-image: linear-gradient(top, #02433F, #012A28);
  }

  #bottom input::-webkit-input-speech-button {
    -webkit-appearance: button;
    background: url("/images/web/mic.png") left center no-repeat;
