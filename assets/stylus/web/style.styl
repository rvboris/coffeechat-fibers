top_height = 50px
bottom_height = 50px
tab_height = 31px

border-radius()
  -moz-border-radius arguments
  -webkit-border-radius arguments
  border-radius arguments

box-shadow()
  -moz-box-shadow arguments
  -webkit-box-shadow arguments
  box-shadow arguments

html
body
  height 100%
  background #0F3044 url(/images/web/bg.png)
  font-family UniversCyrLight, "Gill Sans", Calibri, "Trebuchet MS", sans-serif
  overflow hidden
input, button
  font-family UniversCyrLight, "Gill Sans", Calibri, "Trebuchet MS", sans-serif
#app
  height auto !important
  height 100%
  min-height 100%
  #top
    height top_height - 2px
    overflow hidden
    border-bottom 1px solid #174969
    outline 1px solid #061117
    position relative
    #logo
      margin 8px 0 0 15px
    #login
      position relative
      float right
      height top_height
      line-height top_height
      display inline-block
      min-width 610px
      form
        display block
        height 23px
        line-height 23px
        margin-top 10px
        label
          margin-right 10px
          color #92A6B2
          text-shadow 1px 1px 0 #061117
          font-size 0.9em
        input[type=text], input[type=password]
          height 18px
          margin-right 10px
          padding 1px 2px
          border 1px solid #061117
          box-shadow 1px 1px 0 #174969
          border-radius 3px
          color #748E9C
          font-size 0.8em
          text-shadow 1px 1px 0 #061117
          &:focus
            color #92A6B2
        button[type=submit], button#forgot
          height 23px
          line-height 20px
          margin-left 10px
          padding 1px 5px
          border 1px solid #539223
          border-radius 3px
          color #87C15A
          font-size .75em
          box-shadow 1px 1px 0 #17290A
          text-shadow 1px 1px 0 #17290A
          &:hover
            color #87C15A
          &:active
            border 1px solid #000
            box-shadow 1px 1px 0 #539223
        button#forgot
          border 1px solid #8C7A22
          color #BDB65E
          box-shadow 1px 1px 0 #29250A
          text-shadow 1px 1px 0 #29250A
          &:hover
            color #BDB65E
          &:active
            border 1px solid #000
            box-shadow 1px 1px 0 #6E601B
     button#uLogin, span.oauth
       display inline-block
       float right
     button#uLogin
       width 50px
       height 33px
       margin-top 8px
       margin-right 15px
       background url(/images/web/oauth.png)
       border none
       cursor pointer
     span.oauth
       display inline-block
       padding 0 10px
       float right
       color #92A6B2
       text-shadow 1px 1px 0 #061117
       font-size 0.9em
    #user
      float right
      color #388E9E
      line-height top_height - 3px
      margin-right 10px
      text-shadow 1px 1px 1px #001C1C
      font-size 0.9em
      overflow hidden
      div.status
        display inline-block
        height 25px
        line-height 25px
        padding 0 5px
        margin 0 1px
        border-radius 3px
        span.name
          float left
          color #85BBC9
          margin-right 10px
        span.icon
          background url(/images/web/statuses.png) no-repeat
          width 10px
          height 10px
          display inline-block
          &.online
            background-position 0px center
          &.away
            background-position -10px center
          &.unavailable
            background-position -20px center
        button.switch
          text-decoration none
          display inline-block
          margin-left 5px
          border none
          background none
          width 16px
          height 16px
          color #85BBC9
          font-size 0.8em
          height 25px
          line-height 25px
          text-shadow 1px 1px 1px #001C1C
          &:hover
            color #CCB86B
        div.statuses
          display none
          height 25px
          line-height 25px
          float right
          margin-left 5px
          border-left 1px solid #091C28
          button
            height 25px
            line-height 25px
            margin-right 5px
            text-decoration none
            text-shadow 1px 1px 1px #001C1C
            background none
            span.icon
              margin 0 5px
              float left
              height 25px
            &#online
              .status
                color #62AC2B
                &:hover
                  color #89D34F
              .icon
                background-position 0px center
            &#away
              .status
                color #AEA028
                &:hover
                  color #D6C74D
              .icon
                background-position -10px center
            &#unavailable
              .status
                color #B16125
                &:hover
                  color #D98749
              .icon
                background-position -20px center
            span.status
              float right
              height 25px
              line-height 25px
